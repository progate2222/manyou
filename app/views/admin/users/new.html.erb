<h1>管理者によるユーザー登録</h1>

<%= form_with(model: [:admin, @user], local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <br>
  <%= f.label :admin %>
  <%= f.select :admin ,[["true", "true"], ["false", "false"]] %>
  <br>
  <br>
  <%= f.submit "登録" %>
<% end %>
