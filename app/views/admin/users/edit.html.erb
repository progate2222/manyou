<h1>ユーザー【<%= @user.name%>】の編集</h1>

<%= form_with(model: [:admin, @user], local: true, url: admin_user_path) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <br>
  <%= f.label :admin %>
  <%= f.select :admin ,[["true", "true"], ["false", "false"]] %>
  <br>
  <br>
  <%= f.submit "編集" %>
<% end %>
